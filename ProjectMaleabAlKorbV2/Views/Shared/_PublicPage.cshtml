<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Soccore, Football" />
    <script type="application/x-javascript">
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <title>@ViewBag.Title</title>
    @RenderSection("Contact", false)
    @RenderSection("Style_Reservation", false)
    @Styles.Render("~/template/css")

    <link href="//fonts.googleapis.com/css?family=Source+Sans+Pro:300,300i,400,400i,600,600i,700,900" rel="stylesheet">

    <link rel="icon" href="~/Logo.ico" />


    <script>
        function showNotify() {
            $.notify("Warning: In Progress...", { globalPosition: 'top-center', className: 'info' });
        }
        function showNotifySuccess() {
            $.notify("You're accepted the terms", { globalPosition: 'top-center', className: 'success' });
        }
    </script>
</head>
<body>

    <div class="header">

        <div class="design_header_right">
            <ul>
                <li><i class="fa fa-phone" aria-hidden="true"></i>(+212) 6 78 00 00 11</li>
                <li><i class="fa fa-envelope" aria-hidden="true"></i><a href="mailto:info@mak.com">info@mak.com</a></li>
            </ul>
        </div>
        <div class="design_header_left">
            <ul>
                @{
                    Html.RenderPartial("_loginSingUp");
                }
            </ul>

        </div>
        <div class="clearfix"> </div>
    </div>

    <div class="banner">
        <nav class="navbar navbar-default">
            <div class="navbar-header navbar-left">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <h3><a class="navbar-brand" href="@Url.Action("Index","Home")">Maleab Al Korb</a></h3>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
                <nav class="link-effect-2" id="link-effect-2">
                    <ul class="nav navbar-nav">
                        <li class="active"><a href="@Url.Action("Index","Home")"><span data-hover="Home">Home</span></a></li>
                        <li><a href="@Url.Action("About","Home")"><span data-hover="About Us">About Us</span></a></li>
                        <li><a href="#partners"><span data-hover="Events">Partners</span></a></li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span data-hover="Short Codes">Others</span> <b class="caret"></b>
                            </a>
                            <ul class="dropdown-menu agile_short_dropdown">
                                <li><a href="#glly">Latest gallery</a></li>
                                <li><a href="#latestNews">Latest News</a></li>
                            </ul>
                        </li>
                        <li><a href="@Url.Action("Contact","Home")"><span data-hover="Mail Us">Contact Us</span></a></li>
                    </ul>
                </nav>

            </div>
            <div class="maak_agile_search">
                <form action="#" method="post">
                    <input type="search" name="Search" placeholder="Search Keywords..." required="" />
                    <input type="submit" value="Search">
                </form>

            </div>
        </nav>
    </div>
    <!------------------------------------Content----------------------------------->

    @RenderBody()

    <!------------------------------------Content----------------------------------->

    @{ Html.RenderPartial("_Signup");}
    @{ Html.RenderPartial("_Login");}

    <!---------------------------sign up and sign in---------------------------------->
    <!-- footer -->
    <div class="footer_agile_mak">
        <div class="container">
            <div class="agileits_design_footer_grids">
                <div class="col-md-3 footer-maak-agileits">
                    <h3> Support</h3>
                    <ul>
                        <li>Help</li>
                        <li>FAQ</li>
                        <li>Blog</li>
                    </ul>
                </div>
                <div class="col-md-3 footer-agileits">
                    <h3>Conditions</h3>
                    <ul>
                        <li>Terms of use</li>
                    </ul>
                </div>
                <div class="col-md-3 footer-wthree">
                    <h3>Partners</h3>
                    <ul>
                        <li>Mr. Againan</li>
                        <li>Mr. Ajouil</li>
                        <li>Mr. Samir</li>
                        <li>Mr. Boucham</li>
                    </ul>
                </div>

                <div class="col-md-3 footer-agileits-design">
                    <h3>Our Links</h3>
                    <ul>
                        <li><a href="@Url.Action("Index","Home")">Home</a></li>
                        <li><a href="@Url.Action("About","Home")">About</a></li>
                        <li><a href="#">Events</a></li>
                        <li><a href="@Url.Action("Contact","Home")">Mail Us</a></li>
                    </ul>
                </div>
                <div class="clearfix"></div>

            </div>
            <div class="agileits_design_logo logo2">
                <h2><a href="index.html">Malaeb Al Korb</a></h2>
                <div class="agileits-social">
                    <ul>
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
